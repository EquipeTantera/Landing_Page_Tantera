{
  "kind": "collectionType",
  "collectionName": "about_uses",
  "info": {
    "singularName": "about-us",
    "pluralName": "about-uses",
    "displayName": "About_us",
    "description": "Armazena informações sobre a seção 'Sobre Nós' da atlética, incluindo propósito, fundação, mascote, contatos e diretoria."
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "purpose": {
      "type": "text",
      "required": true
    },
    "foundation": {
      "type": "text",
      "required": true
    },
    "mascot_image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "mascot_description": {
      "type": "text",
      "required": true
    },
    "contact_id": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::contact.contact"
    },
    "board_id": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::board.board"
    }
  }
}
