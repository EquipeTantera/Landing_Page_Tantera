{
  "kind": "collectionType",
  "collectionName": "boards",
  "info": {
    "singularName": "board",
    "pluralName": "boards",
    "displayName": "Board",
    "description": "Modelo para armazenar informações sobre gestões específicas da atlética."
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "description": {
      "type": "text",
      "required": true
    },
    "year": {
      "type": "string",
      "required": true
    },
    "start_date": {
      "type": "date",
      "required": true
    },
    "end_date": {
      "type": "date",
      "required": true,
      "customValidator": "function (value, context) { return value > context.data.start_date; }"
    },
    "current": {
      "type": "boolean",
      "required": true,
      "unique": true
    },
    "specific_board_id": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::specific-board.specific-board",
      "mappedBy": "board_id"
    },
    "members_id": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::member.member"
    },
    "result_id": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::result.result",
      "inversedBy": "board_id"
    },
    "plannings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::planning.planning"
    }
  }
}
