{
  "kind": "collectionType",
  "collectionName": "members",
  "info": {
    "singularName": "member",
    "pluralName": "members",
    "displayName": "member",
    "description": "Modelo para armazenar informações sobre os membros da atlética Tantera."
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": ["images"]
    },
    "role": {
      "type": "string",
      "required": true
    },
    "board": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::board.board",
      "inversedBy": "members_id"
    },
    "specific_board_id": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::specific-board.specific-board",
      "inversedBy": "member_id"
    }
  }
}
