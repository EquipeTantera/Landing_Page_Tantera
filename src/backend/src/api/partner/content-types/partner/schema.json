{
  "kind": "collectionType",
  "collectionName": "partners",
  "info": {
    "singularName": "partner",
    "pluralName": "partners",
    "displayName": "partner",
    "description": "Armazena informações sobre parceiros da atlética, incluindo título, slogan, ícones e associações com eventos e resultados."
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slogan": {
      "type": "string",
      "required": true
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": ["images"]
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": ["images"]
    },
    "result_id": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::result.result"
    },
    "event_id": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::event.event",
      "inversedBy": "partner_id"
    }
  }
}
