{
  "kind": "collectionType",
  "collectionName": "plans",
  "info": {
    "singularName": "plan",
    "pluralName": "plans",
    "displayName": "Plan",
    "description": "Armazena informações sobre os planos de sócios da atlética."
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "advantages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::advantage.advantage",
      "mappedBy": "plan"
    },
    "payment": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::payment.payment",
      "inversedBy": "plans"
    },
    "faqs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::faq.faq",
      "mappedBy": "plan"
    }
  }
}
